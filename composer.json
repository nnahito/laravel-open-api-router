  "description": "Automatically generates OpenAPI files from Laravel's router and FormRequest",
  "autoload": {
    "psr-4": {
      "Nnahito\\LaravelOpenApiRouter\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Nな人",
      "email": "nnahiko@gmail.com"
    }
  ],
  "require": {
    "php": "^8.4",
    "laravel/framework": "^12.0",
    "symfony/yaml": "^7.3"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nnahito\\LaravelOpenApiRouter\\LaravelOpenApiRouterServiceProvider"
      ]
    }
  }
}
